@font-face {
  font-family: "{{fontName}}";
  src:
  /* IE9 */
  url("{{cssFontsUrl}}/{{fontName}}.eot?v={{version}}");
  src:
  /* IE */
  url("{{cssFontsUrl}}/{{fontName}}.eot?v={{version}}#iefix") format("embedded-opentype"),
  /* Chrome, Firefox */
  url("{{cssFontsUrl}}/{{fontName}}.woff2?v={{version}}") format("woff2"),
  url("{{cssFontsUrl}}/{{fontName}}.woff?v={{version}}") format("woff"),
  /* Chrome, Firefox, Opera, Safari, Android, iOS 4.2+ */
  url("{{cssFontsUrl}}/{{fontName}}.ttf?v={{version}}") format("truetype"),
  /* iOS 4.1- */
  url("{{cssFontsUrl}}/{{fontName}}.svg?v={{version}}#{{fontName}}") format("svg");
}

[class*=" {{classPrefix}}"], [class^={{classPrefix}}] {
  font-family: "{{fontName}}" !important;
  speak: none;
  display: inline-block;
  font-style: normal;
  vertical-align: baseline;
  text-align: center;
  text-transform: none;
  line-height: 1;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
